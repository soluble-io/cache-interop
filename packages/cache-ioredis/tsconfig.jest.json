{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": false,
    "declaration": false,
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "isolatedModules": true,
    "rootDir": ".",
    "paths": {
      "@soluble/cache-interop": ["../cache-interop/src"],
      "@soluble/cache-interop/*": ["../cache-interop/src/*"]
    }
  }
}
