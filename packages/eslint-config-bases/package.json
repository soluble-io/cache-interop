{
  "name": "@soluble/eslint-config-bases",
  "version": "1.2.2",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/soluble-io/cache-interop",
  "repository": {
    "type": "git",
    "url": "https://github.com/soluble-io/cache-interop",
    "directory": "packages/eslint-config-bases"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "type": "commonjs",
  "main": "./index.js",
  "exports": {
    ".": {
      "require": "./src/index.js"
    },
    "./patch/modern-module-resolution": {
      "require": "./src/patch/modern-module-resolution.js"
    },
    "./helpers": {
      "require": "./src/helpers/index.js"
    },
    "./graphql-schema": {
      "require": "./src/bases/graphql-schema.js"
    },
    "./jest": {
      "require": "./src/bases/jest.js"
    },
    "./playwright": {
      "require": "./src/bases/playwright.js"
    },
    "./prettier": {
      "require": "./src/bases/prettier.js"
    },
    "./react": {
      "require": "./src/bases/react.js"
    },
    "./rtl": {
      "require": "./src/bases/rtl.js"
    },
    "./regexp": {
      "require": "./src/bases/regexp.js"
    },
    "./sonar": {
      "require": "./src/bases/sonar.js"
    },
    "./storybook": {
      "require": "./src/bases/storybook.js"
    },
    "./typescript": {
      "require": "./src/bases/typescript.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo ./.eslintcache ./node_modules/.cache",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/eslint-config-bases.eslintcache",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "fix-all-files": "eslint --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix"
  },
  "dependencies": {
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-playwright": "^0.12.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.14.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-testing-library": "^5.10.2"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "graphql": "^16.3.0",
    "prettier": "^2.7.1",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0",
    "typescript": "^4.5.2 || ^5.0.2"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/prettier": "2.7.2",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "eslint": "8.37.0",
    "graphql": "16.6.0",
    "prettier": "2.8.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "4.4.1",
    "typescript": "5.0.3"
  }
}
